          var idx = first_idx + Math.floor(rest_len / 2);
          var cur_page_ele = pages[idx].page;
          if (cur_page_ele.offsetTop + cur_page_ele.clientTop + cur_page_ele.clientHeight >= container_min_y) {
            last_idx = idx;
          } else {
            first_idx = idx;
          }
          rest_len = last_idx - first_idx;
        }
        
